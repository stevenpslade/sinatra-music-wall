<h1>Music Wall</h1>

<% @songs.each do |song| %>
  <div class="message">
  <h3><a href="/songs/<%= song.id %>"><%= song.song_title %> </a></h3>
    <p>
      By: <%= song.author %>
    </p>
    <p>
      <a href=<%= song.url %>><%= song.url %></a>
    </p>
  </div>
<% end %>

<a href="/songs/new">Post a song...or don't!</a>
